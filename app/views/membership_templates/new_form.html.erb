<h1>
  Add a new membership
</h1>


<hr>

<form action="/create_membership" method="post">

  <div class="form-group">
    <label for="event_id">
      Event
    </label>

    <input type="text" id="event_id" name="event_id" class="form-control">
  </div>

  <div class="form-group">
    <label for="member_id">
      Member
    </label>

    <input type="text" id="member_id" name="member_id" class="form-control">
  </div>

  <button class="btn btn-block btn-outline-secondary">
    Create membership
  </button>
</form>

<hr>

<a href="/memberships">
  Go back
</a>
